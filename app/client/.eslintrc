{
  "extends": [
    "prettier",
    "airbnb", "airbnb/hooks"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "import",
    "react",
    "react-hooks",
    "jsx-a11y"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-named-as-default": "off"
  }
}